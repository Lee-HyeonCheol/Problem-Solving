#include <bits/stdc++.h>
using namespace std;

string s;

int main(void) {
	while (1) {
		getline(cin, s);
		if (s == "#") break;

		int cnt = 0;
		for (int i = 0; i < s.length(); i++) {
			switch (s[i]) {
			case 'a': case 'A':
			case 'e': case 'E':
			case 'i': case'I':
			case 'o': case 'O':
			case 'u': case 'U':
				cnt++;
			}
		}
		cout << cnt << '\n';
	}
}