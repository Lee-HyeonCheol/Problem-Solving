#include <bits/stdc++.h>
using namespace std;

int N, M;

int main(void) {
	cin.tie(NULL);
	cout.tie(NULL);
	ios_base::sync_with_stdio(false);
	cin >> N >> M;
	vector<int> x(N + M);
	for (int i = 0; i < N + M; i++)
		cin >> x[i];
	sort(x.begin(), x.end());
	for (auto& it : x)
		cout << it << ' ';
}